{
  "name": "@marcuzgabriel/reanimated-burger-menu",
  "author": "Marcuz Gabriel Larsen <marcuzgabriel@gmail.com>",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test:unit": "jest",
    "test:jest:ci": "jest --coverage",
    "test:jest:watch": "jest --watch",
    "format": "prettier --write --list-different ./src/**",
    "lint-diff": "eslint --ext '.js,.ts,.tsx' src/** && yarn prettier --check src/**",
    "web": "cross-env NODE_ENV=development webpack serve --config web/webpack.config.js",
    "web:clean": "rimraf web/vendor/ web/build",
    "web:build": "rimraf web/build/; cross-env NODE_ENV=production webpack --config web/webpack.config.js",
    "type:check": "yarn tsc --noEmit",
    "type:generate:windows": "yarn type:generate:clean && yarn type:generate:create && yarn type:generate:tsc && yarn type:generate:delete:windows && yarn type:generate:copy:windows",
    "type:generate:ios": "yarn type:generate:clean && yarn type:generate:create && yarn type:generate:tsc && yarn type:generate:delete:ios && yarn type:generate:copy:ios",
    "type:generate:clean": "cross-env-shell rimraf lib",
    "type:generate:create": "cross-env-shell mkdir lib",
    "type:generate:tsc": "yarn tsc",
    "type:generate:delete:windows": "cross-env-shell del \"lib\\index.js && cross-env-shell del \"lib\\*.web.js && cross-env-shell && cross-env-shell del \"lib\\ReanimatedBurgerMenu.js",
    "type:generate:delete:ios": "rm ./lib/index.js && rm ./lib/index.web.js && rm ./lib/ReanimatedBurgerMenu.js",
    "type:generate:copy:windows": "cross-env-shell Xcopy \"src\\ReanimatedBurgerMenu.ts\" \"lib\\index.js*\"",
    "type:generate:copy:ios": "rsync -auv ./src/ReanimatedBurgerMenu.ts ./lib/index.js"
  },
  "main": "lib/index.js",
  "module": "lib/index",
  "types": "reanimated-burger-menu.d.ts",
  "react-native": "src/ReanimatedBurgerMenu",
  "files": [
    "src/",
    "lib/",
    "reanimated-burger-menu.d.ts",
    "README.md",
    "!__tests__",
    "!__snapshots__",
    "!*.test.js"
  ],
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-svg": "*",
    "react-native-svg-web": "*",
    "react-native-reanimated": "*",
    "react-native-redash": "*",
    "style-loader": "*",
    "styled-components": "*"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@babel/core": "^7.13.16",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-transform-arrow-functions": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "7.13.0",
    "@babel/runtime": "^7.13.17",
    "@react-native-community/eslint-config": "^3.0.1",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-native": "^0.64.5",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.9",
    "@types/styled-components-react-native": "^5.1.1",
    "@types/setimmediate": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-jest": "^27.0.6",
    "babel-loader": "8.2.1",
    "babel-plugin-module-resolver": "^3.1.3",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "core-js": "^3.11.0",
    "cross-env": "^5.1.4",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.0.6",
    "jest-sonar-reporter": "^2.0.0",
    "metro-react-native-babel-preset": "^0.66.0",
    "prettier": "^2.3.1",
    "process": "^0.11.10",
    "setimmediate": "^1.0.5",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "0.64.2",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "2.3.0-beta.3",
    "react-native-redash": "^16.0.11",
    "react-native-svg": "^12.1.1",
    "react-native-svg-web": "^1.0.9",
    "react-native-web": "0.15.7",
    "react-refresh": "^0.8.3",
    "react-test-renderer": "17.0.1",
    "style-loader": "^0.23.1",
    "styled-components": "^5.0.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.2.3",
    "webpack": "^5.26.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcuzgabriel/reanimated-burger-menu.git"
  }
}
